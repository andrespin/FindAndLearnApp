    android:id="@+id/en_words_graph"
    app:startDestination="@id/en_words">

    <fragment
        android:id="@+id/en_words"
        android:name="android.findandlearnapp.words_manager.en_words.EngWordsFragment"
        android:label="MoviesFragment"
